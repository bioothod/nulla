{
    "endpoints": [
        "0.0.0.0:8080"
    ],
    "backlog": 512,
    "threads": 10,
    "buffer_size": 65536,
	"logger": {
        "level": "notice",
        "frontends": [
            {
                "formatter": {
                    "type": "string",
                    "pattern": "%(timestamp)s %(request_id)s/%(lwp)s/%(pid)s %(severity)s: %(message)s, %(...L)s"
                },
                "sink": {
                    "type": "files",
                    "path": "/dev/stdout",
                    "path1": "nulla.log",
                    "autoflush": true,
                    "rotation": { "move": 0 }
                }
            }
        ]
    },
    "daemon": {
        "fork": false,
        "uid": 1000
    },
    "monitor-port": 21212,
    "request_header": "X-Request",
    "trace_header": "X-Trace",
    "application": {
	"remotes": [
		"localhost:1026:2"
	],
	"buckets": [
		"b1"
	],
	"metadata-groups": [1,2],
	"tmp-dir": "/tmp",
	"hostname": "http://192.168.1.45:8080"
    }
}
